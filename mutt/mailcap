text/plain; vi %s &; copiousoutput; description=Plaintext;

# text/html; w3m %s; nametemplate=%s.html; description=HTML;
# text/html; w3m -dump %s; nametemplate=%s.html; copiousoutput
text/html; w3m %s; nametemplate=%s.html; copiousoutput

application/msword; antiword '%s'; copiousoutput; description=Word Document;
nametemplate=%s.doc

application/pdf; evince '%s'; copiousoutput; description=PDF Document;
nametemplate=%s.pdf

